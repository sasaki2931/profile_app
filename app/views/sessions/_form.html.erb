<%= form_with(scope: :session, url: sessions_path, local: true) do |f| %>
  <div class = "center">
    <div class ="center-inner">
      <div class = "filed">
        <%= f.label :メールアドレス %>
        <%= f.email_field :email, class: "input-underline",id: 'session_email' %>
      </div>
      <div class = "filed">
        <p> <%= f.label :パスワード %></p>
        <p> <%= f.password_field :password, class: "input-underline",id: 'session_password' %></p>
      </div>
    </div>
    <div class ="actions">
      <%= f.submit "ログインする",class:"submit-button" %>
    </div>
  </div>
<% end %>